       <pre>        <span style="color: #0000ff;background:red" title="static">static</span> <span style="color: #0000ff;">void</span> bubble_sort(<span style="color: #0000ff;">int</span><span style="color: #000000;">[] unsorted)
        {
            </span><span style="color: #0000ff;">for</span> (<span style="color: #0000ff;">int</span> i = <span style="color: #800080;">0</span>; i &lt; unsorted.Length; i++<span style="color: #000000;">)
            {
                </span><span style="color: #0000ff;">for</span> (<span style="color: #0000ff;">int</span> j = i; j &lt; unsorted.Length; j++<span style="color: #000000;">)
                {
                    </span><span style="color: #0000ff;">if</span> (unsorted[i] &gt;<span style="color: #000000;"> unsorted[j])
                    {
                        </span><span style="color: #0000ff;">int</span> temp =<span style="color: #000000;"> unsorted[i];
                        unsorted[i] </span>=<span style="color: #000000;"> unsorted[j];
                        unsorted[j] </span>=<span style="color: #000000;"> temp;
                    }
                }
            }
        }

        </span><span style="color: #0000ff;">static</span> <span style="color: #0000ff;">void</span> Main(<span style="color: #0000ff;">string</span><span style="color: #000000;">[] args)
