<body><pre><code class="cpp">#include&lt;stdio.h>
#include&lt;stdbool.h>
bool judge(int a,int b,int c){
	if(<span title="{<,/,>}" style="background:#7CCD7C">a+b>c</span>&&<span title="{<,/,>}" style="background:#7CCD7C">b+c>a</span>&&<span title="{/,/,>}" style="background:#7CCD7C">a+c>b</span>){
		if(<span title="{<,=,>}" style="background:#7CCD7C">a==b</span>){
			if(<span title="{<,=,/}" style="background:#7CCD7C">b==c</span>){
				printf("等边三角形\n");
			}else{
				printf("等腰三角形\n");
			}
		}else{
			if(<span title="{<,/,>}" style="background:#7CCD7C">b==c</span>){
				printf("等腰三角形\n");
			}else{
				if(<span title="{<,/,>}" style="background:#7CCD7C">c==a</span>){
					printf("等腰三角形\n");
				}else{
					printf("不等边三角形\n");
				}
			}
		}
	}else{
		printf("不构成三角形\n");
	}
	return 0;
}
int main(int argc,char *argv[]){
	if(<span title="{/,=,/}" style="background:#7CCD7C">argc&lt;4</span>){
		printf("需要三个参数\n");
		return 0;
	}
	int a=atoi(argv[1]);
	int b=atoi(argv[2]);
	int c=atoi(argv[3]);
	printf("%d",judge(a,b,c));
	return 0;
}
</code>
